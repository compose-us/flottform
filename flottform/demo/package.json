	"version": "0.0.1",
	"private": true,
	"description": "Flottform demo implementation",
	"author": "compose.us GmbH",
	"keywords": [
		"flottform",
		"forms",
		"demo",
		"webrtc"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "pnpm run test:integration && pnpm run test:unit",
