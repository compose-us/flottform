		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@types/leaflet": "^1.9.12",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"@vitejs/plugin-basic-ssl": "^1.1.0",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.56.0",
